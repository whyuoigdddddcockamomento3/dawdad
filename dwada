for i, v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.hotbar["1"]["4"]:GetChildren()) do
        if v:FindFirstChild("1") ~= nil then
            local uicorner = Instance.new("UICorner")
            v["1"]["1"]:Destroy()
            v["1"].BackgroundColor3 = Color3.fromRGB(0,0,0)
            v["1"].BorderSizePixel = 0
            uicorner.Parent = v["1"]
        end
    end
